    "laravel-vite-plugin": "^0.5.3",
    "postcss": "^8.4.6",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "dependencies": {
    "@inertiajs/inertia": "^0.11.0",
