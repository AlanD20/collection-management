{
  "name": "collection-management",
  "description": "A personal collection management to store your favorite items and categorize them in a collection.",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "format:js": "prettier --write resources/src",
    "format:php": "./vendor/bin/pint",
    "serve": "php artisan serve",
    "lang": "php artisan lang:export en && php artisan lang:export es",
    "cache:clear": "php artisan clear && php artisan clear-compiled && php artisan config:clear && php artisan cache:clear && php artisan view:clear && php artisan route:clear && php artisan optimize && composer dump-autoload -o",
    "db:fresh": "php artisan migrate:fresh --seed"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/ziggy-js": "^1.3.2",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.1.12",
    "daisyui": "^2.20.0",
    "laravel-vite-plugin": "^0.5.3",
    "postcss": "^8.4.6",
    "prettier": "^2.7.1",
    "react-icons": "^4.4.0",
    "tailwindcss": "^3.1.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "dependencies": {
    "@inertiajs/inertia": "^0.11.0",
    "@inertiajs/inertia-react": "^0.8.0",
    "@inertiajs/progress": "^0.2.7",
    "@uiw/react-md-editor": "^3.17.2",
    "date-fns": "^2.29.1",
    "laravel-echo": "^1.13.1",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "pusher-js": "^7.4.0",
    "react": "^18.2.0",
    "react-date-picker": "^8.4.0",
    "react-dom": "^18.2.0",
    "react-select": "^5.4.0",
    "zustand": "^4.0.0"
  }
}
